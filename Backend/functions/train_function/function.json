{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "modelOutput",
      "type": "blob",
      "direction": "out",
      "path": "models/model_{wine_type}_{datetime}.pkl",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "scalerOutput",
      "type": "blob",
      "direction": "out",
      "path": "models/scaler_{wine_type}_{datetime}.pkl",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "cleanedOutput",
      "type": "blob",
      "direction": "out",
      "path": "cleaned/cleaned_{wine_type}_{datetime}.csv",
      "connection": "AzureWebJobsStorage"
    }
  ]
}

