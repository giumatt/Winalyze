{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "name": "modelTestOutput",
      "type": "blob",
      "direction": "out",
      "path": "models-test/model_{wine_type}-testing.pkl",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "scalerOutput",
      "type": "blob",
      "direction": "out",
      "path": "models/scaler_{wine_type}.pkl",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "cleanedOutput",
      "type": "blob",
      "direction": "out",
      "path": "cleaned/data_{wine_type}.csv",
      "connection": "AzureWebJobsStorage"
    }
  ]
}

